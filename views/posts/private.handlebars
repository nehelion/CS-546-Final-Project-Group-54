    <button class="open-button" onclick="openForm()">+ Add a Movie</button>
	
    <div class="form-popup" id="myForm">
        <form action="/addmovie" class="form-container">
            <h2>Add a Movie</h2>

            <label for="movie"><b>Movie Name</b></label>
            <input type="text" placeholder="Enter Movie Title" name="email" required>

            <label for="psw"><b>Year Released</b></label>
            <input type="password" placeholder="year released" name="psw" required>

            <button type="submit" class="btn">Add a Movie!</button>
            <button type="button" class="btn cancel" onclick="closeForm()">Close</button>
        </form>
